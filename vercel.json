{
    "version": 2,
    "functions": {
        "server/src/index.ts": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/server/src/index.ts"
        },
        {
            "source": "/assets/:path*",
            "destination": "/web/dist/assets/:path*"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/web/dist/index.html"
        }
    ],
    "cleanUrls": true
}